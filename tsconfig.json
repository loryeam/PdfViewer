{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "allowJs": true,
    "rootDir": "viewer/js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "viewer/js/**/*"
  ]
}
